<h1 class = "category"> Egypt: Sports News </h1>

<!-- Egypt Today -->
<div class="news-feeds">
<%= image_tag ('egypt-today.png'), :class => "news-feed-logo" %>


<?xml version="1.0"?>
<?xml-stylesheet href="application.css"?>

<% url='http://www.ruby-lang.org/en/feeds/news.rss' %>

<% open(url) do |rss| %>

<% feed = RSS::Parser.parse('http://www.egypt-today.com/egypt-football/rss.xml',false) %>

<% puts "feed", feed.inspect %>
<% feed.items.each do |item| %>
  <% puts "item", item.inspect %>
  <h2><%= link_to item.title,item.link  %></h2>
  <p><%= item.pubDate %> </p>
  <p><%= item.description %></p>

<!-- <ul>
    <li class="title"><%= puts "Title: #{feed.channel.title}" %></li>
    <li class="item"><%=  puts "Item: #{item.title}" %></li>
</ul> -->

  <%  end %>
<%  end %>
</xml>

</div>
