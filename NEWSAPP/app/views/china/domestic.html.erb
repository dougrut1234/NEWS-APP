<h1 class = "category"> China: Domestic News </h1>



<!-- china daily  -->

<div class="news-feeds">
<li><%= image_tag ('china-daily.jpg'), :class => "news-feed-logo" %>

<?xml version="1.0"?>
<?xml-stylesheet href="application.css"?>

<% url='http://www.ruby-lang.org/en/feeds/news.rss' %>

<% open(url) do |rss| %>

<% feed = RSS::Parser.parse('http://www.chinadaily.com.cn/rss/china_rss.xml',false) %>

<% puts "feed", feed.inspect %>
<% feed.items.each do |item| %>
  <% puts "item", item.inspect %>
  <h2><%= link_to item.title,item.link  %></h2>
  <p><%= item.pubDate %> </p>
  <p><%= item.description %></p>

  <%  end %>
<%  end %>
</xml>
</div>
