<header class="logo">
  <%= link_to image_tag('logo.jpg'), "/" %>
</header>

<header class="navbar">
   <div class="navbar-class">
     <ul>
       <li><%= link_to "Domestic",   '/venezuela/domestic' %></li>
       <li><%= link_to "World",   '/venezuela/world' %></li>
       <li><%= link_to "Business", '/venezuela/business' %></li>
       <li><%= link_to "Sports", '/venezuela/sports' %></li>
