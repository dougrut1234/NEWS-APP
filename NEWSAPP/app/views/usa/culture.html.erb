<header class="logo">
  <%= link_to image_tag('logo.jpg'), "/" %>
</header>

<header class="navbar">
   <div class="navbar-class">
     <ul>
       <li><%= link_to "Domestic",   '/usa/domestic' %></li>
       <li><%= link_to "World",   '/usa/world' %></li>
       <li><%= link_to "Business", '/usa/business' %></li>
       <li><%= link_to "Sports", '/usa/sports' %></li>
       <li><%= link_to "Technlogy", '/usa/tech' %></li>
       <li><%= link_to "Culture", '/usa/culture' %></li>
     </ul>
     </div>

   </header>
<center>
<h1 class = "category"> United States: Culture News </h1>


<!-- CNN  -->
<div class="news-feeds">
<%= image_tag ('cnn-logo.png'), :class => "news-feed-logo" %>

<?xml version="1.0"?>
<?xml-stylesheet href="application.css"?>

<% url='http://www.ruby-lang.org/en/feeds/news.rss' %>

<% open(url) do |rss| %>

<% feed = RSS::Parser.parse('http://rss.cnn.com/rss/cnn_living.rss',false) %>

<% puts "feed", feed.inspect %>
<% feed.items.each do |item| %>
  <% puts "item", item.inspect %>
  <h2><%= link_to item.title,item.link, :target => "_blank"  %></h2>
  <p><%= item.pubDate %> </p>

<!-- <ul>
    <li class="title"><%= puts "Title: #{feed.channel.title}" %></li>
    <li class="item"><%=  puts "Item: #{item.title}" %></li>
</ul> -->

  <%  end %>
<%  end %>
</xml>

</div>

