<h1 class = "category"> Russia: World News </h1>

<!-- PRAVDA  -->

<% url='http://www.ruby-lang.org/en/feeds/news.rss' %>

<%= open(url) do |rss| %>

<%= feed = RSS::Parser.parse('http://www.pravdareport.com/world/export-articles.xml',false) %>

<%= feed.items.each do |item| %>
    <%= puts "Title: #{feed.channel.title}" %>
    <%=  puts "Item: #{item.title}" %>
  <%  end %>
<%  end %>
